package p2;


public class Math 
{
	public static int sum() {
		int a=20;
		int b=30;
		int c=a+b;
		return c;
		
	}
	public static int mul() {
		int a=10;
		int b=20;
		int c=a*b;
		return c;
	}
	public static int sub() {
		int a=6;
		int b=4;
		int c=a-b;
	return c;
	}
	
	}
